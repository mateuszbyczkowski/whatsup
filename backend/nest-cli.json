{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "tsConfigPath": "tsconfig.app.json",
    "assets": [
      "**/*.json",
      "**/*.md"
    ],
    "watchAssets": true
  },
  "projects": {},
  "monorepo": false,
  "root": ".",
  "entryFile": "main",
  "exec": "node"
}
