<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="whadgest_secure_prefs"/>
        <exclude domain="database" path="whadgest_database"/>
        <exclude domain="database" path="whadgest_database-shm"/>
        <exclude domain="database" path="whadgest_database-wal"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude sensitive data from device transfer -->
        <exclude domain="sharedpref" path="whadgest_secure_prefs"/>
        <exclude domain="database" path="whadgest_database"/>
        <exclude domain="database" path="whadgest_database-shm"/>
        <exclude domain="database" path="whadgest_database-wal"/>
    </device-transfer>
</data-extraction-rules>
